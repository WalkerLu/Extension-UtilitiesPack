{
    "manifestVersion": 1,
    "id": "utilitytasks",
    "name": "Release Management Utility tasks",
    "version": "0.1.9",
    "publisher": "ms-devlabs",
    "description": "Utility tasks for Release Management",
    "public": true,
    "icons": {
        "default": "images/Icon128.png",
        "large": "images/Icon512.png"
    },
    "screenshots": [
        {
            "path": "images/screen1.png"
        },
        {
            "path": "images/screen1.png"
        },
        {
            "path": "images/screen1.png"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "utility",
        "tasks"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/openalm/Extension-UtilitiesPack/blob/master/Utilites/overview.md"
        },
        "issues": {
            "uri": "https://github.com/openalm/Extension-UtilitiesPack/issues"
        },
        "repository": {
            "uri": "https://github.com/openalm/Extension-UtilitiesPack"
        }
    },
    "branding": {
        "color": "#f6f7fb",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "Shell++"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.build_execute",
        "vso.serviceendpoint_manage"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "task-Shellpp",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Shell++"
            }
        }
    ]
}